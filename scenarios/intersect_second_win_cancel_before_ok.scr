
<set>
    request_body = [is_video] ? [default_video_request_body] : [default_request_body]
    response_body = [is_video] ? [default_video_response_body] : [default_response_body]
</set>

<scenario file=register.scr/>

----------------------------------------------------------------------------------------------------
Start a call that will not be answered due to lower call-id
----------------------------------------------------------------------------------------------------
<scenario file=send_invite_partial.scr is_video=[is_video] call_id_param="[min_call_id]"/>


----------------------------------------------------------------------------------------------------
Receive invite from voxip
----------------------------------------------------------------------------------------------------
<recv message=INVITE>
</recv>

<send>
    [default_100_trying]
</send>


----------------------------------------------------------------------------------------------------
Cancel the first call
----------------------------------------------------------------------------------------------------
<scenario file = send_cancel.scr call_number = 1/>


----------------------------------------------------------------------------------------------------
Answer second, incoming call
----------------------------------------------------------------------------------------------------
<send call_number = 2>
    SIP/2.0 200 OK
    [last_Via]
    [last_From]
    [last_To];tag=[tag]
    [last_Call-ID]
    [last_CSeq]
    User-Agent: bb-test
    Contact: <sip:12345678900@[server_ip]:[server_port];transport=[transport]>
    Session-Expires: 300;refresher=uac
    Require: timer
    Supported: replaces,timer,norefersub
    X-Connect-Type: Wifi
    Content-Type: application/sdp
    Content-Length: [len]

    [response_body]
</send>

<recv message=ACK></recv>


